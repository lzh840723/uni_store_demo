{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": "off",
    "unused-imports/no-unused-imports": "error"
  },
  "plugins": ["unused-imports"]
}
